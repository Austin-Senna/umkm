name = "umkm-go-digital"
main = "src/index.js"
compatibility_date = "2024-01-01"

[env.production]
name = "umkm-go-digital-prod"

[env.staging]
name = "umkm-go-digital-staging"

[[kv_namespaces]]
binding = "UMKM_KV"
id = "umkm-business-data"
preview_id = "umkm-business-data-preview"

[env.production.kv_namespaces]
binding = "UMKM_KV"
id = "umkm-business-data-prod"

[env.staging.kv_namespaces]
binding = "UMKM_KV"
id = "umkm-business-data-staging"

[env]
# EdgeOne API Configuration
EDGEONE_API_TOKEN = "your-edgeone-api-token-here"
EDGEONE_ACCOUNT_ID = "your-edgeone-account-id-here"
EDGEONE_ZONE_ID = "your-edgeone-zone-id-here"

# Optional: Custom domain for generated sites
CUSTOM_DOMAIN = "umkm.id"

# Optional: Function URL for internal calls
EDGEONE_FUNCTION_URL = "https://your-function-url.edgeone.com" 